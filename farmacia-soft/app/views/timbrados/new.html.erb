<div class="row">
     <div class="col s5 offset-s3">
         <div class="card-panel">
             <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s10 offset-s1">
                    <div class="card-content white-text">
                       <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
           
            <div class="row">
               <%= simple_form_for(@timbrado,url:timbrado_create_path,html:{autocomplete:'off'},validate: true) do |f| %>
                 <%= render 'layouts/error_messages', f: f %>
                  <h4 class="header2">Timbrado Form</h4>
                 <%= render 'timbrados/partials/fields', f: f %>
               <% end %>
            </div> 
         </div>    
     </div>
</div>  
 <%= render 'configuraciones/partials/show_modal'%>   

