
<div class="row">
     <div class="col s4 offset-s4">
         <div class="card-panel">
              <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s10 offset-s1">
                    <div class="card-content white-text">
                        <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
            <h4 class="header2">Grupo Medicamento Form</h4>
            <div class="row">
               <%= simple_form_for(@grupoMedicamento,url:update_grupoMedicamento_path,html:{autocomplete:'off',method: :put}) do |f| %>
                   <%= render 'layouts/error_messages', f: f%>
                   <%= render 'grupomedicamentos/partials/fields', f: f %>
              <% end %>
            </div>
         </div>
     </div>
</div>
