
<div class="row">
     <div class="col s4 offset-s4">
         <div class="card-panel">
              <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s12">
                    <div class="card-content white-text">
                        <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
            <div class="row">
               <%= simple_form_for(@grupoMedicamento,url:update_grupoMedicamento_path,html:{autocomplete:'off',method: :put}) do |f| %>
                   <%= render 'layouts/error_messages', f: f%>
                   <fieldset>
                     <legend>Grupo Medicamento Form</legend>
                     <%= render 'grupomedicamentos/partials/fields', f: f %>
                  </fieldset>
              <% end %>
            </div>
         </div>
     </div>
</div>
