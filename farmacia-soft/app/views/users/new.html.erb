<div class="row">
     <div class="col s5 offset-s3">
         <div class="card-panel">
             <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s10 offset-s1">
                    <div class="card-content white-text">
                       <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
            <div class="row">
               <%= simple_form_for(@user,url:sign_up_user_path,html:{autocomplete:'off'},:validate => true) do |f| %>
                  <fieldset>
                    <legend>Usuario Form</legend>
                   <%= render 'users/partials/fields', f: f %>
                 </fieldset>
              <% end %>
            </div>
         </div>
     </div>
</div>
