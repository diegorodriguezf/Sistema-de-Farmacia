
<div id ='edit_factura_venta_form 'class="row">
     <div class="col s8 offset-s2">
         <div class="card-panel">
              <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s10 offset-s1">
                    <div class="card-content white-text">
                        <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
            <h4 class="header2">Factura Venta Form</h4>
            <div class="row">
               <%= simple_form_for(@factura_venta,url:update_factura_venta_path,html:{autocomplete:'off',method: :put}) do |f| %>
                   <%= render 'layouts/error_messages', f: f%>
                   <%= render 'facturaventas/partials/header_fields', f: f %>
              <% end %>
            </div>
         </div>
     </div>
</div>
