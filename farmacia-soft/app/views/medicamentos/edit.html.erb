
<div id ='edit_medicamento_form 'class="row">
     <div class="col s6 offset-s3">
         <div class="card-panel">
              <% if flash[:notice]%>
                 <div id="card-alert" class="card green col s10 offset-s1">
                    <div class="card-content white-text">
                        <%= flash[:notice] %>.
                     </div>
                 </div>
              <% end %>
            <div class="row">
               <%= simple_form_for(@medicamento,url:update_medicamento_path,html:{autocomplete:'off',method: :put}) do |f| %>
                   <%= render 'layouts/error_messages', f: f%>
                   <fieldset>
                     <legend>Medicamento Form</legend>
                     <%= render 'medicamentos/partials/fields', f: f %>
                  </fieldset>
              <% end %>
            </div>
         </div>
     </div>
</div>
